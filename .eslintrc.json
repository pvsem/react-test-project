{
    "env": {
        "browser":true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", {"singleQuote": true, "tabWidth": 4, "useTabs": false, "jsxSingleQuote": true}],
        "no-unused-vars": "warn"
    }
}